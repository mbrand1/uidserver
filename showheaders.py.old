@app.route("/")
def showheaders():
	rh = request.headers
	print(rh)
	return jsonify({ 'ip' : request.remote_addr,
					'user-agent' : rh.get('User-Agent'),
					'do-not-track' : rh.get('Dnt'),
					'x-uidh' : rh.get('X-UIDH'),
					'x-acr' : rh.get('X-ACR'),
					'ncc' : rh.get('NCC'),
					})

